{
  "plugins": [
    ["babel-plugin-module-resolver", {
      "root": ["./src"],
      "alias": {
        "api": "./src/api",
        "components": "./src/components",
        "constants": "./src/constants",
        "hooks": "./src/hooks",
        "ui": "./src/ui"
      }
    }]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}